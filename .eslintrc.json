{
    "env": {
        "browser": false,
        "es2020": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "prefer-rest-params": 0,
        "no-invalid-this": 0,
        "max-len": ["error", { "code": 120, "ignoreComments": true }],
        "no-var": "off"
    }
}
